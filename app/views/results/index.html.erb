
<h1> List of Outcomes</h1> 

<p><%= flash[:notice]%> </p>

<%= link_to 'Add a new outcome', new_result_path %>

<table>
  <tr>
   
        <th>Date</th>
        <th>Outcomes</th>
        <th>Completion</th>
     
  </tr>
 
  
    <tr>
    <% @results.each do |result| %>
        <td><%= result.updated_at%></td>
        <td><%= result.outcome %></td>
        <td><%= result.completion %></td>
        <td><%= link_to 'Edit', edit_result_path(result) %></td>
        <td><%= link_to 'Delete', result_path(result), method: :delete, data: { confirm: 'Are you sure you want to delete this?' }%></td>
        </tr>
  <% end %>
</table>


