<div class="row">
  <div class="col-md-11 offset-md-1">
    <h4 class="heading"> School Years (duh) </h4> 

    <p><%= flash[:notice]%> </p>

      <table class="table table-bordered">
        <thead>
          <tr>
            <th scope="col">Year</th>
            <th scope="col"=>Edit</th>
            <th scope="col"=>Delete</th>
          </tr>
        </thead>
        <tbody>
          <tr>
          <% @results.each do |result| %>
            <td><%= result.outcome %></td>
            <td><%= link_to 'Edit', edit_result_path(result) %></td>
            <td><%= link_to 'Delete', result_path(result), method: :delete, data: { confirm: 'Are you sure you want to delete this?' }%></td>
          </tr>
        <% end %>
      </table>
    <%= link_to 'Add a new school year (for real?)', new_result_path %>
  </div>
</div>


